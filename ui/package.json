{
  "name": "singularity-ui",
  "version": "20.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-static",
    "storybook:serve": "npx serve -s storybook-static -l 9001",
    "test": "jest --config=jest.config.cjs --runInBand",
