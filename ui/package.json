  },
  "dependencies": {
    "framer-motion": "latest",
    "lucide-react": "latest",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "autoprefixer": "^10",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5",
    "@playwright/test": "^1.45.0",
    "playwright": "^1.45.0",
    "@storybook/react": "^7.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "serve": "^14.0.0",
    "jest": "^29.7.0",
    "babel-jest": "^29.7.0",
    "@babel/core": "^7.22.9",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0"
